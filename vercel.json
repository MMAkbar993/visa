{
  "version": 2,
  "functions": {
    "**/*.php": {
      "runtime": "vercel-php@0.5.2"
    }
  },
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*\\.(jpg|jpeg|png|gif|svg|ico|css|js|woff|woff2|ttf|eot))",
      "dest": "/$1",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "rewrites": [
    {
      "source": "/privacy-policy",
      "destination": "/privacy-policy/index.php"
    },
    {
      "source": "/terms-and-conditions",
      "destination": "/terms-and-conditions/index.php"
    },
    {
      "source": "/thankyou",
      "destination": "/thankyou/index.php"
    },
    {
      "source": "/(.*)",
      "destination": "/index.php"
    }
  ]
}
